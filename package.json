{
  "name": "factry-historian-datasource",
  "version": "2.0.3-beta",
  "description": "A datasource plugin for Factry Historian",
  "scripts": {
    "build": "webpack -c ./.config/webpack/webpack.config.ts --env production",
    "build:dev": "webpack -c ./.config/webpack/webpack.config.ts --env development",
    "dev": "webpack -w -c ./.config/webpack/webpack.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ignore-path ./.gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "e2e": "playwright test",
    "server": "docker-compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest"
  },
  "author": "Factry",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@grafana/eslint-config": "^6.0.1",
    "@grafana/tsconfig": "1.2.0-rc1",
    "@swc/core": "1.3.75",
    "@swc/helpers": "^0.5.11",
    "@swc/jest": "^0.2.36",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^12.1.5",
    "@types/debounce-promise": "^3.1.9",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.4",
    "@types/node": "^18.19.33",
    "@typescript-eslint/parser": "^5.62.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.11.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-webpack-plugin": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "glob": "^10.4.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^2.8.8",
    "replace-in-file-webpack-plugin": "^1.0.6",
    "sass": "1.63.2",
    "sass-loader": "13.3.1",
    "style-loader": "3.3.3",
    "swc-loader": "^0.2.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "4.8.4",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-livereload-plugin": "^3.0.2"
  },
  "resolutions": {
    "rxjs": "7.3.0"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@grafana/data": "~9.4.17",
    "@grafana/plugin-e2e": "^1.2.1",
    "@grafana/runtime": "~9.4.17",
    "@grafana/schema": "~9.4.17",
    "@grafana/ui": "~9.4.17",
    "debounce-promise": "^3.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tslib": "2.5.3"
  },
  "packageManager": "pnpm@8.6.2"
}
