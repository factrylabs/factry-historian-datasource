{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/master/docs/sources/developers/plugins/plugin.schema.json",
  "type": "datasource",
  "name": "Factry Historian Datasource",
  "id": "factry-historian-datasource",
  "metrics": true,
  "backend": true,
  "executable": "gpx_factry-historian-datasource",
  "alerting": true,
  "annotations": true,
  "info": {
    "description": "A datasource plugin for Factry Historian",
    "author": {
      "name": "Factry",
      "url": "https://www.factry.io"
    },
    "keywords": ["historian", "factry"],
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "links": [
      {
        "name": "Website",
        "url": "https://factry.io"
      },
      {
        "name": "License",
        "url": "https://github.com/factrylabs/factry-historian-grafana-datasource/blob/main/LICENSE"
      }
    ],
    "screenshots": [
      {
        "name": "Asset property query example",
        "path": "img/grafana-datasource-assets.png"
      },
      {
        "name": "Measurement query example",
        "path": "img/grafana-datasource-measurements.png"
      },
      {
        "name": "Event query example",
        "path": "img/grafana-datasource-events.png"
      }
    ],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },
  "dependencies": {
    "grafanaDependency": ">=9.4.0",
    "plugins": []
  }
